                "value": "# from langflow.field_typing import Data\nfrom langflow.custom import Component\nfrom langflow.io import MessageTextInput, Output, SecretStrInput\nfrom langflow.schema import Data\nimport os\nimport base64\nimport google.generativeai as genai\n\n\nclass CustomComponent(Component):\n    display_name = \"Custom Component\"\n    description = \"Use as a template to create your own component.\"\n    documentation: str = \"http://docs.langflow.org/components/custom\"\n    icon = \"code\"\n    name = \"CustomComponent\"\n\n    inputs = [\n        MessageTextInput(\n            name=\"input_value\",\n            display_name=\"Image File Path\",\n            info=\"Provide the file path of the image.\",\n            value=\"\",\n            tool_mode=True,\n        ),\n        MessageTextInput(\n            name=\"prompt\",\n            display_name=\"Prompt\",\n            info=\"Ask on the image.\",\n            value=\"\",\n            tool_mode=True,\n        ),\n        SecretStrInput(\n            name=\"api_key\",\n            display_name=\"GEMINI API Key\",\n            info=\"The GEMINI API Key to use for the GOOGLE model.\",\n            advanced=False,\n            value=\"GEMINI_API_KEY\",\n        ),\n    ]\n\n    outputs = [\n        Output(display_name=\"Text\", name=\"text\", method=\"build_output\"),\n    ]\n\n    def build_output(self) -> Data:\n        # Configure the Gemini API\n        genai.configure(api_key=self.api_key)\n\n        # Initialize the model\n        model = genai.GenerativeModel(model_name=\"gemini-1.5-pro\")\n\n        # Get the image file path from input\n        image_path = self.input_value\n\n        # Define your prompt\n        prompt = self.prompt\n\n        # Process the single image\n        response = process_single_image(image_path, model, prompt)\n        \n        return response\n\n\ndef process_single_image(image_path, model, prompt):\n    # Check if the file exists and is an image\n    if not os.path.isfile(image_path):\n        return {\"error\": \"Invalid file path or file does not exist.\"}\n\n    # List of common image file extensions\n    valid_extensions = ['.jpg', '.jpeg', '.png', '.gif', '.bmp', '.webp']\n    if os.path.splitext(image_path)[1].lower() not in valid_extensions:\n        return {\"error\": \"The provided file is not a supported image format.\"}\n\n    try:\n        # Read and encode the image data\n        with open(image_path, 'rb') as image_file:\n            image_data = base64.b64encode(image_file.read()).decode('utf-8')\n            image_content = {\n                'mime_type': f'image/{os.path.splitext(image_path)[1][1:].lower()}',\n                'data': image_data\n            }\n        \n        # Add the prompt to the content\n        input_data = [image_content, prompt]\n\n        # Generate the response using the model\n        response = model.generate_content(input_data)\n        return response.text\n\n    except Exception as e:\n        return {\"error\": f\"An error occurred while processing the image: {e}\"}\n",
